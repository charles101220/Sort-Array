import java.util.Arrays;

class SortArray {
	public static void main(String args[])
	{
		int[] arr = { 2, -2, 10, 713, 99 , -42, 676 ,-50 , -75 };
		System.out.println("The original array is: ");
		for (int num : arr) {
			System.out.print(num + " ");
		}
		Arrays.sort(arr);
		System.out.println("\nThe sorted array is: ");
		for (int num : arr) {
			System.out.print(num + " ");
		}
	}
}
